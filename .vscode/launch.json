  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "M2 WS Replay",
      "program": "${workspaceFolder}\\bin\\m2-ws-replay",
      "args": [
        "${workspaceFolder}/logs/m2-1572225274187.log"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "M2 Serial Replay",
      "program": "${workspaceFolder}\\bin\\m2-serial-replay",
      "args": [
        "COM6",
        "${workspaceFolder}/logs/m2-1572225274187.log"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start Server",
      "program": "${workspaceFolder}\\bin\\www",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Parse DBC",
      "program": "${workspaceFolder}\\bin\\parse-dbc",
      "args": [".\\dbc\\tesla_model3.dbc"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Onyx M2 Monitor",
      "program": "${workspaceFolder}\\bin\\onyx-m2-monitor"
    }

  ]
}